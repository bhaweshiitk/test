
public class Person {
	
	private int age;
	private String name;
	//access_sp retrn_type fun_name(){}
	public void display() {
	
		System.out.println("name:-"+name+"\t"+"age:-"+age);
	}
	//constructer below
	public Person()
	{
		age=10;
		name="Bhawesh";
	}
	public  Person(int a, String n) {
		age=a;
		name=n;
	}
}
