
public class Date {

	// TODO Auto-generated method stub
	private int date;
	private int month;
	private int year;
    
	public void display() {
		
		System.out.println(date+" "+month+" "+year);
	}

	public int getDate() {
		return date;
	}

	public void setDate(int date) {
		this.date = date;
	}

	public int getMonth() {
		return month;
	}

	public void setMonth(int month) {
		this.month = month;
	}

	public int getYear() {
		return year;
	}

	public void setYear(int year) {
		this.year = year;
	}
	
		
	

}
