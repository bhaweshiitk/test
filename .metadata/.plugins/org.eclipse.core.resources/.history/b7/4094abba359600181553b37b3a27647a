
public class Person {
	
	private int age;
	private String name;
	//access_sp retrn_type fun_name(){}
	public void display() {
	
		System.out.println("name:-"+name+"\t"+"age:-"+age);
	}
	//constructer below
	public Person()
	{
		age=10;
		name="Bhawesh";
	}
	public  Person(int age, String name) {
		this.age=age;
		this.name=name;
	}
	public void setAge(int a) {
		age=a;
	}
	public void setName(String s) {
		name=s;
	}
}
